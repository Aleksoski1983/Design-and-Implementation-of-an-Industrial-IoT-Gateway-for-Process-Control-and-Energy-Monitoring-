        {
          "dataset": "iiot_gateway",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "\r\nSELECT\r\n    MAX(CASE WHEN mv.browse_name = 'Sim_SupplyTemp_GT71_01' THEN sd.field_value END) AS Sim_SupplyTemp_GT71_01,\r\n    MAX(CASE WHEN mv.browse_name = 'Sim_ExhaustTemp_GT72_01' THEN sd.field_value END) AS Sim_ExhaustTemp_GT72_01,\r\n    MAX(CASE WHEN mv.browse_name = 'Sim_ReturnTemp_GT81_01' THEN sd.field_value END) AS Sim_ReturnTemp_GT81_01,\r\n    MAX(sd.timestamp) as timestamp\r\nFROM sensor_data sd\r\nJOIN opcua_monitored_variables mv\r\n    ON sd.measurement = mv.measurement_name\r\nWHERE mv.browse_name IN ('Sim_SupplyTemp_GT71_01', 'Sim_ExhaustTemp_GT72_01','Sim_ReturnTemp_GT81_01');\r\n\r\n",
          "refId": "A",
          "sql": {
